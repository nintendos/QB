<% include top %>


    <div class="panel panel-default draggable">
      <div class="panel-body">



<div class="container-fluid">
  <div class="row-fluid">
    <h1>后台管理</h1>
  <% if (status) { %>  
    <h3>管理员列表</h3>
    <% if (adminlist.length) { %>  
      <table class="table table-hover span12">
        <thead>
        <tr>
          <th>#</th>
          <th>用户名</th>
          <th>密码</th>
          <th>年龄</th>
        </tr>
        </thead>
        <tbody>
        <% adminlist.forEach(function(data){ %>
          <tr>
            <td><%= data.id %></td>
            <td><%= data.user %></td>
            <td><%= data.password %></td>
            <td><%= data.age %></td>
          </tr>
        <% }) %>  
        </tbody>
      </table>
    <% } %>  
  <% } else {%>
    <strong>无数据</strong>
  <% } %>  
  </div>
</div>



      </div>
      </div>

        

<% include footer %>
<% include bottom %>